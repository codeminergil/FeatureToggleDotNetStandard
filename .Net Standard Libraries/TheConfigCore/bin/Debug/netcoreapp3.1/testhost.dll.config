<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="ToggleConfiguration" type="TheConfigStandard.XmlConfiguration.ToggleConfigurationSection, TheConfigStandard"/>
  </configSections>
  <appSettings>
    <add key="Toggle:Enabled" value="true"/>
    <add key="Toggle:DefaultValue" value="false"/>

    <add key="ToggleFlag:CacheInheritableDatasource" value="true" />
    <add key="ToggleFlag:StrongToggleId" value="true" />
  </appSettings>
  <ToggleConfiguration>
    <toggles>
      <toggle name="CacheInheritableDatasource" enabled="true">
        <roles>
          <role name="Staff" />
        </roles>
        <users>
          <user name="abcd" />
        </users>
        <ipaddresses>
          <ipaddress value="127.0.0.1/28" />
        </ipaddresses>
      </toggle>
      <toggle name="StrongToggleId" enabled="true" />
    </toggles>
  </ToggleConfiguration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <system.diagnostics>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <add name="ConsoleListener" type="System.Diagnostics.ConsoleTraceListener" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>